import java.awt.Button;


public class mybutton extends Button {

	int value;
	public mybutton(int value){
		super(""+value);
		this.value=value;
		
		this.setFont(new Font("",Font.BOLD,34));
		
	}
	public mybutton(){
		super("");
		this.value=0;
		
		this.setFont(new Font("",Font.BOLD,34));
		this.setSize(value, value);
		
	}
	public int getValue() {
		return value;
	}
	public void setValue(int value) {
		this.value = value;
	}
}
